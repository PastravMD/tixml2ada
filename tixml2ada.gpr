
with "xmlada";

project tixml2Ada is

   for Source_Dirs use ("src");
   for Main use ("tixml2ada.adb");
   for Object_Dir use "obj";
   for Exec_Dir use ".";

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-O0", "-gnatwa", "-gnatQ", "-gnatw.X");
   end Compiler;

end tixml2Ada;

